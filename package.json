{
  "name": "commobile",
  "version": "1.0.0",
  "description": "commobile: An Ionic project",
  "dependencies": {
    "bower": "^1.3.3",
    "cucumber": "^0.4.4",
    "glob": "^4.0.6",
    "gulp": "^3.5.6",
    "gulp-concat": "^2.2.0",
    "gulp-minify-css": "^0.3.0",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^0.7.1",
    "gulp-util": "^2.2.14",
    "ionic": "~1.2.8",
    "jscs": "^1.7.3",
    "jshint": "^2.5.8",
    "karma": "^0.12.24",
    "karma-jasmine": "*",
    "karma-junit-reporter": "*",
    "karma-phantomjs-launcher": "*",
    "phantomjs": "~1.9.12",
    "protractor": "~1.3.0",
    "protractor-http-mock": "https://github.com/AlliterativeAnimals/protractor-http-mock/tarball/0eaafa5dfc76a7bdffd96c39b06da7d02f4720aa",
    "shelljs": "^0.3.0",
    "colors": "~1.0.3"
  },
  "scripts": {
    "install": "npm run bower && npm run update-webdriver",
    "bower": "node_modules/bower/bin/bower install",
    "update-webdriver": "node_modules/protractor/bin/webdriver-manager update",
    "pretest": "npm run serve-start && npm run selenium-start",
    "test": "{ npm run jshint && npm run jscs && npm run karma && npm run protractor-standalone; } 2>/dev/null",
    "posttest": "npm run serve-stop && npm run selenium-stop",
    "jscs": "node_modules/jscs/bin/jscs www/js/**/*.js",
    "jshint": "node_modules/jshint/bin/jshint www/js/**/*.js",
    "karma": "node_modules/karma/bin/karma start test/karma.conf.js --single-run --no-color",
    "test-nokarma": "{ npm run jshint && npm run jscs; } 2>/dev/null",
    "serve-start": "node_modules/ionic/bin/ionic serve --nolivereload --nobrowser --nolivereload --port 8100 >/dev/null 2>&1 & echo $! >> ionic.pid",
    "serve-stop": "if [ -f ionic.pid ]; then kill `cat ionic.pid`; rm ionic.pid; fi;",
    "selenium-start": "node_modules/protractor/bin/webdriver-manager start >/dev/null 2>&1 & echo $! >> selenium.pid",
    "selenium-stop": "if [ -f selenium.pid ]; then kill `cat selenium.pid`; rm selenium.pid; fi;",
    "preprotractor-standalone": "npm run serve-start && npm run selenium-start",
    "protractor-standalone": "node_modules/protractor/bin/protractor test/protractor.standalone.config.js",
    "postprotractor-standalone": "npm run serve-stop && npm run selenium-stop",

    "watch-karma": "node_modules/karma/bin/karma start test/karma.conf.js",
    "watch-protractor": "node_modules/grunt-cli/bin/grunt start-protractor"
  },
  "devDependencies": {
    "grunt-protractor-runner": "~1.1.4",
    "grunt-cli": "~0.1.13",
    "grunt-contrib-watch": "~0.6.1",
    "grunt": "~0.4.5",
    "grunt-protractor-webdriver": "https://github.com/AlliterativeAnimals/grunt-protractor-webdriver/tarball/6241ccca78f119e9eb1c8fa27d0c1f2d9caa2127",
    "load-grunt-tasks": "~1.0.0",
    "grunt-shell": "~1.1.1"
  }
}
